style=\"color: black\" @click=\"openDetails\"> 暂无</div>\n    <EarlyWarningDialog :open=\"open\" :title=\"title\" :form=\"form\" @submitFormParent=\"submitFormParent\" @cancelParent=\"cancelParent\" />\n  </div>\n</template>\n\n<script>\n\nimport EarlyWarningDialog from \"@/components/EarlyWarningDialog\"\n\nexport default {\n  components: {EarlyWarningDialog},\n  name: 'index',\n  data () {\n    return {\n      // 预警信息弹窗-是否显示弹出\n      open: false,\n      // 预警信息弹窗-弹出title标题\n      title: \"\",\n      // 预警信息弹窗-表单字段\n      form: {\n        name: '',\n        event: '',\n        status: '0',\n        date: '',\n        remark: ''\n      },\n    }\n  },\n  methods: {\n    toMainIndex () {\n      this.$router.push({ name: 'main' })\n    },\n    // 实时预警list点击事件-dialog表单数据初始化\n    openDetails(){\n      console.log(\"点击事件row数据为 status ：1\")\n      this.open = true;\n      this.title = \"预警信息\";\n      this.form.name = \"name\";\n      this.form.event = \"事件\";\n      this.form.status = \"1\";\n      this.form.date = \"2021-08-17 21:28:40\";\n    },\n    // 实时预警list点击事件-提交按钮\n    submitFormParent() {\n      console.log('show 确定'+ this.form.name + this.form.status+this.form.event+this.form.remark);\n      this.open = false;\n      this.reset();\n    },\n    // 实时预警list点击事件-取消按钮\n    cancelParent() {\n      this.open = false;\n      this.reset();\n    },\n    // 实时预警lsit点击事件-表单数据重置\n    reset() {\n      this.form = {\n        name: undefined,\n        event: undefined,\n        status: '0',\n        date: undefined,\n        remark: undefined\n      };\n    },\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n  .div_basic {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    margin-top: 20px;\n    width: 100%;\n    border: 2px;\n    height: 70px;\n    background: $main-color;\n  }\n</style>"]}]}